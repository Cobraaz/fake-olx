{
  "name": "interview",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "rimraf ./build && tsc",
    "build:prod": "tsc",
    "client:dev": "cd client/ && npm start",
    "server:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "gen-env": "npx gen-env-types .env -o src/@types/env.d.ts -e .",
    "heroku:postbuild:client": "cd client && npm install && npm run build",
    "heroku-postbuild": "concurrently \"npm run build:prod\" \"npm run heroku:postbuild:client\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.1.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.31",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
